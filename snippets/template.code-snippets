	"template": {
		"prefix": ["$$template", "$$tmpl"],
		"scope": "ogone",
		"description": "start writing the component's template",
		"body": [
			"<template>",
			"  ${1:<!-- nodetypes 1, 3, 8 supported -->}",
			"</template>",
		]
	},
	"recycle template": {
		"prefix": ["$$template-recycle", "$$tmplr"],
		"scope": "ogone",
		"description": "start recycling a component",
		"body": [
			"<template is=\"$1\">",
			"  ${2:<!-- nodetypes 1, 3, 8 supported -->}",
			"</template>",
		]
	},
}